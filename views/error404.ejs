<%- include partials/head %>
<%- include partials/header %>

<section id="error-page">
  <div class="error-page-inner">
    <div class="container-fluid">
      <div class="row">
        <div class="col-sm-12">
          <div class="text-center">
            <div class="bg-404">
              <div class="error-image">
                <img class="img-responsive" src="images/404.png" alt="">
              </div>
            </div>
            <h2>PAGE NOT FOUND</h2>
            <p>We searched to the Moon &amp; back, but could not find what you were looking for. <br>
            The page might have moved or had its name changed.</p>
            <a href="/" class="btn btn-error">RETURN TO THE HOMEPAGE</a>
            <div class="social-link">
              <span><a href="#"><i class="fa fa-facebook"></i></a></span>
              <span><a href="#"><i class="fa fa-twitter"></i></a></span>
              <span><a href="#"><i class="fa fa-google-plus"></i></a></span>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
<script>
  console.error('<%- message %> \n', '<%- err.status %> \n', '<%- err.stack %> \n');
  // Redirect to Homepage
  setTimeout(function() {
    window.location.replace('/')
  }, 10000)
</script>

<%- include partials/footer %>
<%- include partials/foot %>